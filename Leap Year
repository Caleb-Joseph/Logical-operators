#include <iostream>
using namespace std;
int main() {
  int year;
  cout<< "Enter a year";
  cin>>year;
  if (year > 9999 && year > 999){
    cout<<"Your year is a four-digit number\n";
  }
  else{
    cout<<"Your year is not a four-digit number\n";
  }
  if (year %4 == 0 || year %400 ==0){
    cout<<"Your year is a leap year\n";
  }
  else if(year % 100 == 0 && year % 400 != 0){
    cout<<"Your year is not a leap year\n";
  }
  else{
    cout<<"Your year is not a leap year\n";
  }
}
